{
   "compilerOptions": {
      "outDir": "./dist",
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strict": false,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
   },
   "include": ["src/**/*"]
}
