{
  "$id": "favorite.json",
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "required": [
    "id",
    "title",
    "url",
    "ingredients",
    "instructions"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "ingredients": {
      "type": "array",
      "items": {
        "text": "string"
      }
    },
    "instructions": {
      "type": "array",
      "items": {
        "text": "string"
      }
    }
  }
}
